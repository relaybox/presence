{
  "family": "relaybox-metrics-prod",
  "taskRoleArn": "arn:aws:iam::905418171520:role/livetabs-prod-ecs-roles-ECSTaskRole-hru7miQceI2K",
  "containerDefinitions": [
    {
      "name": "relaybox-metrics-prod",
      "essential": true,
      "memoryReservation": "256",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/aws/ecs/relaybox-metrics-prod",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "/aws/ecs"
        }
      }
    }
  ]
}

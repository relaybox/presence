{
  "family": "relaybox-presence-dev",
  "taskRoleArn": "arn:aws:iam::767397814726:role/relaybox-dev-ecs-roles-ECSTaskRole-ODOaUvU1yjQJ",
  "executionRoleArn": "arn:aws:iam::767397814726:role/relaybox-dev-ecs-roles-ECSTaskExecutionRole-rnz8nPpR6cwm",
  "containerDefinitions": [
    {
      "name": "relaybox-presence-dev",
      "image": "${IMAGE}",
      "essential": true,
      "memoryReservation": 256,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/aws/ecs/relaybox-presence-dev",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "/aws/ecs"
        }
      },
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "${AWS_REGION}"
        },
        {
          "name": "DB_HOST",
          "value": "${DB_HOST}"
        },
        {
          "name": "DB_NAME",
          "value": "${DB_NAME}"
        },
        {
          "name": "DB_PORT",
          "value": "${DB_PORT}"
        },
        {
          "name": "DB_IDLE_TIMEOUT_MS",
          "value": "${DB_IDLE_TIMEOUT_MS}"
        },
        {
          "name": "DB_MAX_CONNECTIONS",
          "value": "${DB_MAX_CONNECTIONS}"
        },
        {
          "name": "DB_USERNAME",
          "value": "${DB_USERNAME}"
        },
        {
          "name": "RABBIT_MQ_CONNECTION_STRING",
          "value": "${RABBIT_MQ_CONNECTION_STRING}"
        },
        {
          "name": "RABBIT_MQ_QUEUE_COUNT",
          "value": "${RABBIT_MQ_QUEUE_COUNT}"
        },
        {
          "name": "REDIS_HOST",
          "value": "${REDIS_HOST}"
        },
        {
          "name": "REDIS_PORT",
          "value": "${REDIS_PORT}"
        }
      ],
      "secrets": [
        {
          "name": "DB_PASSWORD",
          "valueFrom": "${DB_PASSWORD_ARN}"
        },
        {
          "name": "RDS_ROOT_CERTIFICATE",
          "valueFrom": "${RDS_ROOT_CERTIFICATE_ARN}"
        }
      ]
    }
  ]
}
